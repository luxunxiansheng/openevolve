{
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "Debug OpenEvolve Run",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/openevolve-run.py",
            "console": "integratedTerminal",
            "args": [
                "${workspaceFolder}/examples/circle_packing_with_artifacts/initial_program.py",
                "${workspaceFolder}/examples/circle_packing_with_artifacts/evaluator.py",
                "--config",
                "${workspaceFolder}/examples/circle_packing_with_artifacts/config_phase_1.yaml",
                "--iterations",
                "1000"
            ],
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}